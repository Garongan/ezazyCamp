{
  "expo": {
    "name": "eazyCamp",
    "slug": "eazyCamp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/eazy-camp.png",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to upload guarantee photos."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/eazy-camp.png",
        "backgroundColor": "#fff8ee"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}