{
  "expo": {
    "name": "eazyCamp",
    "slug": "eazyCamp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/eazy-camp.png",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to upload guarantee photos."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/eazy-camp.png",
        "backgroundColor": "#fff8ee"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.alvindotri.eazyCamp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "05b9b78c-2bbe-4459-b238-cd3b55e3e599"
      }
    }
  }
}
