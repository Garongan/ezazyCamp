{
  "expo": {
    "name": "eazyCamp",
    "slug": "eazyCamp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/eazy-camp.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/eazy-camp.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff8ee"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fff8ee"
      }
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fff8ee"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}